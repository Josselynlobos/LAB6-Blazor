@page "/"

<div class="main">
    <h1>Pizzas</h1>
    <ul class="cards">
        @if (specials != null){
            @foreach (var special in specials){
                <li style="background-image: url('@special.ImageURL')">
                    <div class="info">
                        <span class="price">@special.GetFormattedBasePrice()</span>
                    </div>
                </li>
            }
        }
    </ul>
</div>

@code {
    List<PizzaSpecial> specials = new();

    protected override void OnInitialized(){
        specials.AddRange(new List<PizzaSpecial>{
            new PizzaSpecial { Name = "The baconatorizor", BasePrice = 11.99M, Description="It has Every kind of bacon", ImageURL= data:image/jpeg;base64,}
            new PizzaSpecial { Name = "Buffalo chiken", BasePrice = 12.75M, Description="It has Every kind of bacon", ImageURL= data:image/jpeg;base64,}
            new PizzaSpecial { Name = "Veggie Delight", BasePrice = 11.5M, Description="It has Every kind of bacon", ImageURL=data:image/jpeg;base64,}
            new PizzaSpecial { Name = "Margherite", BasePrice = 9.99M, Description="It has Every kind of bacon", ImageURL=data:image/jpeg;base64,}
            new PizzaSpecial { Name = "Basic Cheese Pizza", BasePrice = 11.99M, Description="It has Every kind of bacon", ImageURL= data:image/jpeg;base64,}
            new PizzaSpecial { Name = "Classic pepperoni", BasePrice = 10.5M, Description="It has Every kind of bacon", ImageURL= data:image/jpeg;base64,}
        })
    }
}